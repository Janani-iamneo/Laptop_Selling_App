@model List<dotnetapp.Models.BeautySpa>

@{
    ViewBag.Title = "Beauty Spa Dashboard";
}

<h2>Beauty Spa Dashboard</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Category</th>
            <th>Price</th>
            <th>Duration (minutes)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var beautySpa in Model)
        {
            <tr>
                <td>@beautySpa.Name</td>
                <td>@beautySpa.Description</td>
                <td>@beautySpa.Category</td>
                <td>@beautySpa.Price</td>
                <td>@beautySpa.DurationInMinutes</td>
            </tr>
        }
    </tbody>
</table>
